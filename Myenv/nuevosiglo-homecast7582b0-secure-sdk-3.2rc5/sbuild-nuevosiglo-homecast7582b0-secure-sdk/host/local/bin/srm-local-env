#!/bin/sh
set -a
. ${SRM_ENV}
PATH=${BUILD_ROOT}/bin:${BUILD_ROOT}/usr/bin:${VANILLA_PATH}
CPATH=${BUILD_ROOT}/usr/include
LIBRARY_PATH=${LD_LIBRARY_PATH}
COMPILER_PATH=${BUILD_ROOT}/usr/bin
unset CPLUS_INCLUDE_PATH C_INCLUDE_PATH
unset CC CXX CXXFILT CPP AR AS NM LD RANLIB STRIP
unset HOST_CC HOST_CFLAGS HOST_CXX HOST_CXXFILT HOST_CPP
unset HOST_AR HOST_AS HOST_NM HOST_LD HOST_RANLIB HOST_STRIP
unset ARCH MAKEFLAGS DESTDIR
unset CROSS_COMPILE cross_compiling
set +a
