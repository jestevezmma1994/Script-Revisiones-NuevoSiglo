            <param tag="fontSizeRatio"><double>0.26</double></param>
            <param tag="textAlign"><string>center</string></param>
        </widget>

        <widget name="infoLong">
            <param tag="xOffsetRatio"><double>0</double></param>
            <param tag="yOffsetRatio"><double>0.646</double></param>
            <param tag="widthRatio"><double>1</double></param>
            <param tag="heightRatio"><double>0.240</double></param>
            <param tag="textColor"><color>efefefff</color></param>
            <param tag="fontSizeRatio"><double>0.18</double></param>
            <param tag="textAlign"><string>center</string></param>
            <param tag="multiLine"><bool>true</bool></param>
        </widget>

        <widget name="time">
            <param tag="time0"><int>16</int></param>
            <param tag="time1"><int>8</int></param>
            <param tag="time2"><int>8</int></param>
        </widget>

	<widget name ="gauge">
	    <param tag="xOffsetRatio"><double>0.067</double></param>
	    <param tag="yOffsetRatio"><double>0.559</double></param>
	    <param tag="widthRatio"><double>0.878</double></param>
	    <param tag="heightRatio"><double>0.062</double></param>
