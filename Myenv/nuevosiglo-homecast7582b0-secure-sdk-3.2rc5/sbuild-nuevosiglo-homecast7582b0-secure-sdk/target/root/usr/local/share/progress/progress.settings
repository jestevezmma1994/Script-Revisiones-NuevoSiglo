    <xi:include href="TextFont.settings"/>
    <widget name="main">
        <common>
            <param tag="font"><ref constant="TextFont"/></param>
        </common>

        <widget name="mainbg">
            <param tag="widthRatio"><double>1</double></param>
            <param tag="heightRatio"><double>1</double></param>
            <param tag="bgColor"><color>4f4f4fff</color></param>
	    <param tag="bg">
                <bitmap><path>bg_mainbg.png</path></bitmap>
            </param>
        </widget>

        <widget name="newVersion">
            <param tag="xOffsetRatio"><double>0.5</double></param>
            <param tag="yOffsetRatio"><double>0.896</double></param>
            <param tag="widthRatio"><double>0.433</double></param>
            <param tag="heightRatio"><double>0.092</double></param>
            <param tag="textColor"><color>efefefff</color></param>
            <param tag="fontSizeRatio"><double>0.41</double></param>
            <param tag="textAlign"><string>right</string></param>
        </widget>

        <widget name="oldVersion">
            <param tag="xOffsetRatio"><double>0.067</double></param>
            <param tag="yOffsetRatio"><double>0.896</double></param>
            <param tag="widthRatio"><double>0.433</double></param>
            <param tag="heightRatio"><double>0.092</double></param>
            <param tag="textColor"><color>efefefff</color></param>
            <param tag="fontSizeRatio"><double>0.41</double></param>
            <param tag="textAlign"><string>left</string></param>
        </widget>

        <widget name="info">
            <param tag="xOffsetRatio"><double>0</double></param>
            <param tag="yOffsetRatio"><double>0.083</double></param>
            <param tag="widthRatio"><double>1</double></param>
            <param tag="heightRatio"><double>0.208</double></param>
