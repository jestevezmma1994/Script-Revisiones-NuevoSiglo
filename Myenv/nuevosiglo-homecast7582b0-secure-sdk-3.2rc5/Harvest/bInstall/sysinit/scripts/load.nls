#!/bin/sh

for module in \
        nls_cp1250 \
        nls_cp1251 \
        nls_cp437 \
        nls_cp737 \
        nls_cp775 \
        nls_cp850 \
        nls_cp852 \
        nls_cp855 \
        nls_cp865 \
        nls_cp866 \
        nls_cp869 \
        nls_iso8859-1 \
        nls_iso8859-13 \
        nls_iso8859-2 \
        nls_iso8859-7 \
        nls_utf8 \
    ; do
        /sbin/modprobe -q -- "${module}"
    done
